{
  "manifest_version": 2,
  "name": "CSS ClassName Exporter",
  "version": "1.0.0",
  "description": "Export unique class names from a web page to a CSV file",
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "96": "icon-96.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "activeTab",
    "downloads",
    "storage",
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": {
      "16": "icon-16.png",
      "32": "icon-32.png",
      "96": "icon-96.png",
      "128": "icon-128.png"
    },
    "default_popup": "popup.html",
    "default_title": "Export Class Names"
  }
}